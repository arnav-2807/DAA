#include <stdio.h>

void insertionSort(int arr[], int n)
{
    for (int i = 1; i < n; ++i) {
        int key = arr[i];
        int j = i - 1;

        while (j >= 0 && arr[j] > key) {
            arr[j + 1] = arr[j];
            j = j - 1;
        }
        arr[j + 1] = key;
    }
}

void printArray(int arr[], int n)
{
    for (int i = 0; i < n; ++i)
        printf("%d ", arr[i]);
    printf("\n");
}

int main()
{
    int arr[] = { 53, 11, 79, 2, 38, 66 };
    int n = sizeof(arr) / sizeof(arr[0]);

    printf("Original array : ");
    printArray(arr, n);

    insertionSort(arr, n);

    printf("Sorted array : ");
    printArray(arr, n);

    return 0;
}

// Output 
/* Original array : 53 11 79 2 38 66 
Sorted array : 2 11 38 53 66 79 */
